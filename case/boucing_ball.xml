<?xml version="1.0" encoding="UTF-8"?>
<sspaceex xmlns="http://www-verimag.imag.fr/xml-namespaces/sspaceex" version="0.2" math="SpaceEx">
  <component id="system">
    <param name="x" type="real" local="false" d1="vx" d2="1" dynamics="any" />
    <param name="y" type="real" local="false" d1="vy" d2="1" dynamics="any" />
    <param name="vx" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="vy" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="t" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="e1" type="label" local="false" />
    <location id="1" name="v1">
      <invariant>y&gt;=-5</invariant>
      <flow>x:=0.1*vx &amp; y:=0.1*vy &amp; vy:=-0.1 &amp; t:=1</flow>
    </location>
    <transition source="1" target="1">
      <label>e1</label>
      <guard>vy&lt;0 &amp; y&lt;=5 </guard>
      <assignment>vy:=-0.75*vy</assignment>
    </transition>
  </component>
</sspaceex>
