<?xml version="1.0" encoding="UTF-8"?>
<sspaceex xmlns="http://www-verimag.imag.fr/xml-namespaces/sspaceex" version="0.2" math="SpaceEx">
	<component id="system">
		<param name="x" type="real" local="false" d1="vx"  dynamics="any" />
		<param name="a" type="real" local="false" d1="vy"  dynamics="any" />
		<param name="t" type="real" local="false" d1="1"  dynamics="any" />
		<param name="e1" type="label" local="false" />
		<param name="e2" type="label" local="false" />
		<location id="1" name="v1">
			<flow>t:=1 &amp;x:=a</flow>
			<invariant>x&lt;=40</invariant>
		</location>
		<location id="2" name="v2">
			<flow>t:=1 &amp;x:=a</flow>
			<invariant>x&gt;=-20</invariant>
		</location>
		<transition source="1" target="2">
			<label>e1</label>
			<guard>x&gt;=20</guard>
			<assignment>a:=-2</assignment>
		</transition>
		<transition source="2" target="1">
			<label>e2</label>
			<guard>x&lt;=0</guard>
			<assignment>a:=2</assignment>
		</transition>
</component>
</sspaceex>
